{
  "editor.fontSize": 13.5,
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.defaultProfile.osx": "/bin/bash",
  "editor.tabSize": 2,
  "cSpell.userWords": ["gravatar", "searchresult"],
  "liveServer.settings.donotShowInfoMsg": true,
  "explorer.confirmDelete": false,
  "editor.wordWrap": "on",
  "editor.formatOnSave": true,
  "prettier.singleQuote": true,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "emmet.syntaxProfiles": {
    "javascript": "jsx",
    "javascript": "html"
  },
  "editor.wordWrapColumn": 50,
  "explorer.confirmDragAndDrop": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "gitlens.currentLine.scrollable": false,
  "liveSassCompile.settings.formats": [
    {
      "format": "compressed",
      "extensionName": ".min.css",
      "savePath": "/dist/css"
    }
  ],
  "liveSassCompile.settings.generateMap": false,
  "workbench.iconTheme": "vscode-icons",
  "vsicons.dontShowNewVersionMessage": true,
  "kite.showWelcomeNotificationOnStartup": false,
  "python.linting.pylintArgs": ["--load-plugins", "pylint-flask"],
  "appService.showSavePrompt": false,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "C_Cpp.updateChannel": "Insiders",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.detectIndentation": false,
    "editor.defaultFormatter": null,
    "editor.insertSpaces": true,
    "editor.tabSize": 4
  },
  "python.formatting.blackArgs": ["--line-length", "79"],
  "python.formatting.provider": "black",
  "azureFunctions.showProjectWarning": false,
  "jupyter.sendSelectionToInteractiveWindow": false,
  "asa.enableFeedbackPrompt": false,
  "tabnine.experimentalAutoImports": true,
  "aws.profile": "profile:default",
  "yaml.customTags": [
    "!Equals sequence",
    "!FindInMap sequence",
    "!GetAtt",
    "!GetAZs",
    "!ImportValue",
    "!Join sequence",
    "!Ref",
    "!Select sequence",
    "!Split sequence",
    "!Sub",
    "!And",
    "!And sequence",
    "!If",
    "!If sequence",
    "!Not",
    "!Not sequence",
    "!Equals",
    "!Or",
    "!Or sequence",
    "!FindInMap",
    "!Base64",
    "!Join",
    "!Cidr",
    "!Sub sequence",
    "!ImportValue sequence",
    "!Select",
    "!Split"
  ],
  "workbench.editor.wrapTabs": true,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "yaml.schemas": {},
  "redhat.telemetry.enabled": false
}
